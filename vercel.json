{
    "version": 2,
    "functions": {
        "api/index.js": {
            "includeFiles": "public/**"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/static/(.*)",
            "dest": "/public/static/$1",
            "continue": true
        },
        {
            "src": "/manifest.json",
            "dest": "/public/manifest.json",
            "continue": true
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico",
            "continue": true
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/((?!api/).*)",
            "dest": "/public/index.html"
        }
    ]
}