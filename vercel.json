{
    "version": 2,
    "functions": {
        "api/index.js": {
            "includeFiles": "public/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        },
        {
            "source": "/static/:path*",
            "destination": "/public/static/:path*"
        },
        {
            "source": "/manifest.json",
            "destination": "/public/manifest.json"
        },
        {
            "source": "/favicon.ico",
            "destination": "/public/favicon.ico"
        },
        {
            "source": "/robots.txt",
            "destination": "/public/robots.txt"
        },
        {
            "source": "/rerendet-logo.png",
            "destination": "/public/rerendet-logo.png"
        },
        {
            "source": "/hero-product.png",
            "destination": "/public/hero-product.png"
        },
        {
            "source": "/logo1920.png",
            "destination": "/public/logo1920.png"
        },
        {
            "source": "/logo512.png",
            "destination": "/public/logo512.png"
        },
        {
            "source": "/((?!api/).*\\..*)",
            "destination": "/public/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/public/index.html"
        }
    ]
}